.special {
  padding: 28px
}

.special>.item {
  position: relative;
  width: calc(100vw - (28px * 2));
  height: calc(100vw - (28px * 2));
  margin-top: 28px
}

.special>.item:first-child {
  margin-top: 0
}

.special>.item:nth-child(3n-2)>.thumbnail>.img {
  background-color: #98847d
}

.special>.item:nth-child(3n-1)>.thumbnail>.img {
  background-color: #a06441
}

.special>.item:nth-child(3n)>.thumbnail>.img {
  background-color: #645e66
}

.special>.item>.thumbnail {
  width: 100%;
  height: 100%
}

.special>.item>.thumbnail>.img {
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 12px
}

.special>.item>.word {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  text-align: center
}

.special>.item>.word>.title {
  font-size: 29px;
  font-weight: 700;
  color: #fff
}

.special>.item>.word>.info {
  margin-top: 12px;
  font-size: 14px;
  color: #fff
}

.special>.item>.btn {
  position: absolute;
  bottom: 38px;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  width: 146px;
  height: 58px;
  text-align: center;
  line-height: 58px;
  font-size: 14px;
  font-weight: 500;
  color: #292929;
  background-color: #fff;
  border-radius: 12px
}

