.bg-tree {
  background-color: #98f498;
}

.warp {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  height: 100%;
}

.dis_but {
  display: flex;
  width: 80%;
  margin: 0 auto;
  justify-content: space-between;
}

.rect {
  width: 100%;
  height: 100vh;
  border-radius: 20rpx;
  padding: 25rpx 25rpx 80rpx 25rpx;
  padding-top: 20vh;
  box-sizing: border-box;
  background-color: #fff;
}

.closeImg {
  width: 100%;
  height: 200rpx;
  margin: 10rpx auto 30rpx;
}

.closeImg image {
  height: 200rpx;
}

.cu-bar .cu-avatar :first-child {
  margin-left: -11px;
}

.mainBox {
  padding: 60rpx 40rpx 20rpx 40rpx;
}

.explainMain {
  width: 100%;
  text-align: left;
  font-size: 24rpx;
  color: #999999;
  margin-top: 30rpx;
}

.mentalTitle {
  margin: 0 0 30rpx 0;
}

.mentalList {
  width: 100%;
  height: 78rpx;
  line-height: 78rpx;
  padding-right: 20rpx;
  border-radius: 12rpx;
  margin: 30rpx 0;
  overflow: hidden;
  border: 1px solid #dddddd;
}

.mentalList .indexBox {
  width: 70rpx;
  height: 78rpx;
  text-align: center;
  line-height: 78rpx;
  float: left;
  background-color: #dddddd;
  margin-right: 15rpx;
}

.red {
  color: red;
  border: 1px solid red;
}

.red .indexBox {
  background-color: red;
  color: #fff
}

.db_but {
  position: fixed;
  height: 40px;
  width: 100%;
  bottom: 0px;
  background: linear-gradient(to top right, #f4c998, #d69c63);
  color: #fff;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}

.mentalListActive {
  width: 100%;
  height: 78rpx;
  line-height: 78rpx;
  padding-right: 20rpx;
  border-radius: 12rpx;
  margin: 30rpx 0;
  overflow: hidden;
  border: 1px solid #0081ff;
  color: #0081ff;
}

.mentalListActive .indexBox {
  background-color: #0081ff;
  color: #ffffff;
  width: 70rpx;
  height: 78rpx;
  text-align: center;
  line-height: 78rpx;
}

.contaier {
  background-color: #f2f2f2;
}

.contaier .bannerBox {
  width: 750rpx;
}

.contaier .bannerBox image {
  width: 750rpx;
}

.contaier .explain {
  width: 750rpx;
  bottom: 40rpx;
  font-size: 24rpx;
  margin: 10rpx 0 40rpx 0;
  text-align: center;
  color: #999999;
}

.contaier .explain .integral {
  font-size: 30rpx;
  margin-bottom: 15rpx;
  color: #333333;
}