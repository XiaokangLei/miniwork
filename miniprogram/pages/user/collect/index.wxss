}
.home-list-left {
  position: relative;
  width: 124px;
  height:70px;
}
.loding{
  margin-top: 20px;
  text-align: center;
  line-height: 30px;
  color:#909399
}

.home-list-left>view:nth-child(1) {
  width: 124px;
  height: 70px;
  border-radius: 3px;
  position: absolute;
}

.home-list-left>view:nth-child(2) {
  width: 124px;
  height: 26px;
  text-align: center;
  background: rgba(255, 255, 255, 0.8);
  padding: 3px;
  padding-left: 6px;
  font-size: 13px;
  font-family: PingFang;
  color: rgba(51, 51, 51, 1);
  position: absolute;
  bottom: 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;

}

.home-jfg button {
  width: 100%;
  height: auto;
  background: #fff;
  margin: 0px;
  display: inline;
  position: initial;
  padding: 0px;
  color: rgb(246 150 4);
  margin-top: -5px;
  line-height: initial;
