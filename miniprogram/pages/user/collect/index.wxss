.detection-main {
  background: rgba(255, 255, 255, 1);
  border-radius: 6px;

}
.servic_top{

  padding-top: 14px;
  background: linear-gradient(to top, #1D4259, #3963BC);


  display: flex;
  color:#Fff;
  padding: 15px 15px;
  padding-top: 10px;
  border-radius: 0px 0px 10px 10px;
  justify-content: space-between;
}
